@model List<HM13OA.Model.WorkflowStep>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ApproveList</title>
</head>
<body>
    <div>
        <ul>
            @if (Model.Count > 0)
            {
                foreach (var item in Model)
                {
                    <li>@Html.ActionLink(item.WorkflowInstance.InstanceTitle, "BaoxiaoApprove", new
                        {
                            id = item.StepId
                        })</li>
                }
            }
            else
            {
                <h1>暂无需要审核的工作流</h1>
            }
        </ul>
    </div>
</body>
</html>
